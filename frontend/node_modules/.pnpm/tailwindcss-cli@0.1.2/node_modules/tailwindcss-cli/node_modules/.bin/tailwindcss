#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/viniciusdamasceno/Desktop/Plataforma/frontend/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6/node_modules/tailwindcss/lib/node_modules:/Users/viniciusdamasceno/Desktop/Plataforma/frontend/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6/node_modules/tailwindcss/node_modules:/Users/viniciusdamasceno/Desktop/Plataforma/frontend/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6/node_modules:/Users/viniciusdamasceno/Desktop/Plataforma/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/viniciusdamasceno/Desktop/Plataforma/frontend/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6/node_modules/tailwindcss/lib/node_modules:/Users/viniciusdamasceno/Desktop/Plataforma/frontend/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6/node_modules/tailwindcss/node_modules:/Users/viniciusdamasceno/Desktop/Plataforma/frontend/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6/node_modules:/Users/viniciusdamasceno/Desktop/Plataforma/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6/node_modules/tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6/node_modules/tailwindcss/lib/cli.js" "$@"
fi
